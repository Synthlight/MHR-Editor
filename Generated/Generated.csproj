		<Compile Remove="Structs\MHR\**"/>
	</ItemGroup>
	<!-- RE4 -->
	<ItemGroup Condition="'!$(Configuration.StartsWith(`RE4`))'">
		<Compile Remove="Enums\RE4\**"/>
		<Compile Remove="Structs\RE4\**"/>
