	</ItemGroup>
	<!-- MHR -->
	<ItemGroup Condition="'!$(Configuration.StartsWith(`MHR`))'">
		<Compile Remove="Enums\MHR\**"/>
		<Compile Remove="Manual\MHR\Structs\**"/>
